{
	"name": "sveltekit-starterkit",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"prisma": {
		"seed": "node --require tsconfig-paths/register prisma/seed.js"
	},
	"scripts": {
		"start": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"docker": "docker compose up -d",
		"format": "prettier --plugin-search-dir . --write .",
		"inlang:lint": "npx @inlang/cli lint --project project.inlang",
		"inlang:translate": "npx @inlang/cli machine translate --project project.inlang",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"postinstall": "paraglide-js compile --project ./project.inlang",
		"preview": "vite preview",
		"setup-dev": "prisma generate && prisma migrate dev && prisma db seed",
		"test:unit": "vitest"
	},
	"dependencies": {
		"@inlang/paraglide-js-adapter-sveltekit": "^0.3.0",
		"@prisma/client": "^5.3.1",
		"bcryptjs": "^2.4.3",
		"bits-ui": "^0.18.1",
		"clsx": "^2.1.0",
		"formsnap": "^0.4.4",
		"leaflet": "^1.9.3",
		"leaflet.markercluster": "^1.5.3",
		"lucide-svelte": "^0.295.0",
		"mode-watcher": "^0.1.2",
		"react-image-file-resizer": "^0.4.8",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.1.20",
		"tiny-invariant": "^1.3.1",
		"uuidv4": "^6.2.13"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.2.1",
		"@inlang/paraglide-js-adapter-vite": "^1.2.5",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/leaflet": "^1.9.1",
		"@types/leaflet.markercluster": "^1.5.2",
		"@typescript-eslint/eslint-plugin": "^6.7.3",
		"@typescript-eslint/parser": "^6.7.3",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.34.0",
		"postcss": "^8.4.21",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prisma": "^5.3.1",
		"sass": "^1.56.1",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-headless-table": "^0.17.5",
		"sveltekit-superforms": "^1.13.4",
		"tailwindcss": "^3.2.4",
		"tsconfig-paths": "^4.0.0",
		"tslib": "^2.4.1",
		"typescript": "^5.2.2",
		"vite": "^5.0.0",
		"vitest": "^1.0.4",
		"zod": "^3.22.4"
	}
}
